<?xml version="1.0" encoding="UTF-8"?>
<!-- dulko.template.exb -->
<!-- Version 5.0 -->
<!-- Andreas Nolda 2019-05-05 -->

<basic-transcription>
  <head>
    <!-- for the metadata variables cf. Granger/Paquot (2017) -->
    <meta-information>
      <project-name>Deutsch-ungarisches Lernerkorpus (Dulko), Universit√§t Szeged</project-name><!-- "corpus_title", "distributor" -->
      <transcription-name><!-- filename without extension --></transcription-name><!-- "text_ID" -->
      <ud-meta-information>
        <!-- document: -->
        <ud-information attribute-name="text_title"><!-- heading of the learner text, if any --></ud-information>
        <ud-information attribute-name="date"><!-- date in ISO form --></ud-information>
        <ud-information attribute-name="country">Hungary</ud-information>
        <ud-information attribute-name="institution">University of Szeged</ud-information>
        <ud-information attribute-name="task_type"><!-- "in-class activity"/"examination" --></ud-information>
        <ud-information attribute-name="task_instructions"><!-- exercise instructions --></ud-information>
        <ud-information attribute-name="written_task"><!-- "translation"/"argumentative essay" --></ud-information>
        <ud-information attribute-name="written_ref_tools">no</ud-information>
        <ud-information attribute-name="task_document"><!-- "yes"/"no" --></ud-information>
        <ud-information attribute-name="task_document_ID"><!-- filename without extension --></ud-information>
        <!-- corpus: -->
        <ud-information attribute-name="corpus_title">Deutsch-ungarisches Lernerkorpus</ud-information>
        <ud-information attribute-name="corpus_acronym">Dulko</ud-information>
        <ud-information attribute-name="distributor">University of Szeged</ud-information>
        <ud-information attribute-name="availability">free of charge</ud-information>
        <ud-information attribute-name="license">CLARIN PUB+BY+SA+PRIV</ud-information>
        <ud-information attribute-name="character_markup">UTF-8</ud-information>
        <ud-information attribute-name="markup_language">XML</ud-information>
        <ud-information attribute-name="corpus_size"><!-- tokens of the current text --></ud-information><!-- filled in by "exb2exb.xsl" --><!-- summed up by "exb2metadata.xsl" -->
        <ud-information attribute-name="corpus_mode">written</ud-information>
        <ud-information attribute-name="editorial_decisions">punctuation normalised to ASCII characters</ud-information>
        <ud-information attribute-name="transcription_guidelines">Dulko-Handbuch</ud-information>
        <ud-information attribute-name="written_process">no</ud-information>
        <ud-information attribute-name="longitudinal">no</ud-information>
        <ud-information attribute-name="field">general language</ud-information>
        <ud-information attribute-name="comp_data">no</ud-information>
        <ud-information attribute-name="annotation">no</ud-information><!-- updated by "exb2exb.xsl" -->
        <ud-information attribute-name="annotation_complete">no</ud-information>
        <ud-information attribute-name="pos_tagged">no</ud-information><!-- updated by "exb2exb.xsl" -->
        <ud-information attribute-name="pos_tagset"><!-- "STTS" --></ud-information><!-- filled in by "exb2exb.xsl" -->
        <ud-information attribute-name="parsed">no</ud-information>
        <ud-information attribute-name="error_annotated">no</ud-information><!-- updated by "exb2exb.xsl" -->
        <ud-information attribute-name="error_annotation_tool"><!-- "EXMARaLDA (Dulko)" --></ud-information><!-- filled in by "exb2exb.xsl" -->
        <ud-information attribute-name="annotation_other"><!-- "lemmata", "sentence spans", "target hypotheses", "differences", "translated text" --></ud-information><!-- filled in by "exb2exb.xsl" -->
      </ud-meta-information>
      <comment></comment>
      <transcription-convention>Punctuation was normalised to ASCII characters. For details cf. the Dulko-Handbuch.</transcription-convention><!-- "editorial_decisions", "transcription_guidelines" -->
    </meta-information>
    <speakertable>
      <speaker id="SPK0">
        <abbreviation></abbreviation><!-- "learner_ID" -->
        <sex value="u"/><!-- "gender" -->
        <languages-used>
          <language lang="deu"/><!-- "L2_language" (corpus) -->
        </languages-used>
        <l1>
          <language lang="hun"/><!-- "L1_language" (corpus), "L1" (learner) -->
          <!-- ... --><!-- "L1" (learner) -->
        </l1>
        <l2>
          <language lang="deu"/><!-- ignored -->
          <!-- ... --><!-- "L2_other_1" -->
        </l2>
        <ud-speaker-information>
          <!-- document: -->
          <ud-information attribute-name="learner_status"><!-- "L2 learner"/"trainee translator" --></ud-information>
          <ud-information attribute-name="age"><!-- years (integer) --></ud-information>
          <ud-information attribute-name="learner_proficiency_level_type">internally rated</ud-information>
          <ud-information attribute-name="learner_proficiency_rating_scale">onSET<!-- or "TestDaF" --></ud-information>
          <ud-information attribute-name="learner_proficiency"><!-- rating (integer) --></ud-information>
          <ud-information attribute-name="learner_level_CEFR_conversion"><!-- "B2"/"C1" --></ud-information>
          <ud-information attribute-name="study_level"><!-- "undergraduate"/"graduate" --></ud-information>
          <ud-information attribute-name="study_area"><!-- "German studies"/"translation studies"/"German teacher training" --></ud-information>
          <ud-information attribute-name="L2_study_years"><!-- years (integer) --></ud-information>
          <ud-information attribute-name="L2_study_institutions"><!-- "primary school", "secondary school", "language school", "private lessons", "university" --></ud-information>
          <ud-information attribute-name="time_spent_L2_country"><!-- months (integer) --></ud-information>
          <ud-information attribute-name="L2_region"><!-- Bundesland, Kanton --></ud-information>
          <!-- corpus: -->
          <ud-information attribute-name="L2_target">mono_L2</ud-information>
          <ud-information attribute-name="proficiency_level">intermediate</ud-information>
          <ud-information attribute-name="proficiency_level_type">learner-based</ud-information>
          <ud-information attribute-name="proficiency_level_descriptors">onSET<!-- or "TestDaF" --></ud-information>
          <ud-information attribute-name="official_language_testing">no</ud-information>
        </ud-speaker-information>
        <comment></comment>
      </speaker>
    </speakertable>
  </head>
  <basic-body>
    <common-timeline>
      <tli id="T0"/>
      <tli id="T1"/>
    </common-timeline>
    <tier id="TIE0" speaker="SPK0" category="word" type="t" display-name="[word]">
    </tier>
  </basic-body>
  <tierformat-table>
    <tier-format tierref="EMPTY-EDITOR">
      <property name="bg-color">#REEGEEBEE</property>
    </tier-format>
    <tier-format tierref="ROW-LABEL">
      <property name="font-size">18</property>
      <property name="font-name">Sans</property>
      <property name="bg-color">#REEGEEBEE</property>
      <property name="text-alignment">Left</property>
    </tier-format>
    <tier-format tierref="COLUMN-LABEL">
      <property name="font-size">12</property>
      <property name="font-name">Sans</property>
      <property name="bg-color">#REEGEEBEE</property>
    </tier-format>
    <tier-format tierref="TIE0">
      <property name="font-name">Sans</property>
      <property name="font-face">Bold</property>
      <property name="font-size">18</property>
    </tier-format>
  </tierformat-table>
</basic-transcription>
